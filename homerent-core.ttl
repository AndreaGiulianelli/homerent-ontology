                :HeatingPrice
                :ReachablePlace
                :RentalAd
                :RentalAdInterest
                :RentalPrice
                :ServicePrice
                :StrategicPoint
                :WaterPrice
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( schema:CollegeOrUniversity
                schema:ExerciseGym
                schema:Hospital
                schema:Museum
                schema:Pharmacy
                :PublicTransport
                :SharedMobility
                :Supermarket
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :AirConditioner
                :Dishwasher
                :Dryer
                :ElectricFan
                :Freezer
                :Fridge
                :Microwave
                :Oven
                :Stove
                :TV
                :Vacuum
